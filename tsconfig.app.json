/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "src",
    "paths": {
      "@app-envs/*": [
        "environments/*"
      ],
      "@app-core/*": [
        "app/core/*"
      ],
      "@app-models/*": [
        "app/core/models"
      ],
      "@app-shared/*": [
        "app/shared/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
